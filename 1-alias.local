# shellcheck shell=ksh
# vim: syntax=sh et ts=2 sw=2 sts=2

if [[ $KSH_VERSION == *MIRBSD* ]]; then
  alias -d \
    dev=~/dev/git                  \
    ports=~/dev/git/freebsd/ports  \
    doc=~/dev/git/freebsd/ports    \
    src=~/dev/git/freebsd/src      \
    pad=/scratch                   \
    scripts=~/.local/script        \
    books=~/.local/share/handbooks
fi
